<main class="container overflow-auto">
    <div class="row justify-content-center my-3">
        <div class="col  col-lg-8 col-xl-6">
            <input type="search" class="form-control  mb-2 searchInput" id="search-input" placeholder="type here to search">
            <div class="btn-div text-center">
                <button  id="search-btn">Search</button>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col" id="table-container">
            <table class="table table-striped table-hover" id="table">
                <thead>
                    <tr class="table-dark ">
                        <% for ( let header of headers ) { %>
                            <th scope="col">
                                <%=header%>
                            </th><%}%>
                    </tr>
                </thead>
                <tbody id="t-body">
                    <% for ( let rowObj of dataArray ) { %>
                        <tr>
                            <% for ( let item in rowObj ) { %>
                                <td class="">
                                    <%=rowObj[item] %>
                                </td>
                                <%}%>
                        </tr>
                        <%}%>
                </tbody>
            </table>
        </div>
    </div>
    <footer>
        <nav aria-label="Page navigation example">
            <ul class="pagination ">
              <li class="page-item searchInput  <%=paginationObj.prevClass %>"><a href ="/view-file?fileId=<%=fileId%>&page=<%=pageNo-1%>">Previous</a></li>
              <li class="page-item searchInput active"><a  href="#"><%=pageNo%></a></li>
              <li class="page-item searchInput <%=paginationObj.nextClass%> "><a  href="/view-file?fileId=<%=fileId%>&page=<%=parseInt(pageNo)+1%>">Next</a></li>
            </ul>
          </nav>
    </footer>


    <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.js"></script>
    
    <script src="js/viewFile.js"></script>
</main>
